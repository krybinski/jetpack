{
    "name": "jetpack",
	"type": "project",
	"description": "Simple and modern MVC framework",
    "license": "MIT",
    "authors": [{
        "name": "Kamil Rybinski"
	}],
	"require": {
        "php": ">=7.0",
        "phpmailer/phpmailer": "^6.0",
        "symfony/http-foundation": "^4.2",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
    },
    "autoload": {
        "classmap": ["app/"],
        "files": [
            "app/helpers.php",
            "config/app.php",
            "config/constants.php",
            "config/database.php",
            "config/mail.php"
        ]
    }
}
