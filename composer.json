{
 	"name": "jetpack",
 	"description": "Simple PHP framework",
	"license": "MIT",
	"type": "project",
	"authors": [{
		"name": "Kamil Rybinski"
	}],
	"require": {
		"php": ">=7.1",
		"pecee/simple-router": "^4.2",
 	 	"vlucas/phpdotenv": "^3.3",
 	 	"twig/twig": "^2.11"
	},
	"require-dev": {},
	"config": {
		"preferred-install": "dist"
	},
    "autoload": {
		"psr-4": {
			"Jetpack\\": "app/"
		},
		"files": [
			"config/app.php",
			"config/database.php",
			"config/mail.php"
		]
    }
}
